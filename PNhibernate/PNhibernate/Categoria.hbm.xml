<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping 
		xmlns="urn:nhirbenate-mapping-2.2"
		assembly="PNhibernate"
		namespace="Serpis.Ad">
	<class name="categoria">
		<id name="id" unsaved-value="0"/>
